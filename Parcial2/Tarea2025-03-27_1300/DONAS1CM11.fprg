                            <output expression="&quot;tu total es de: &quot; &amp; (precioDONAS*numDONAS)-numDONAS*2 &amp; &quot; pesos!&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;tu total es de: &quot; &amp; (precioDONAS*numDONAS)-numDONAS*1 &amp; &quot; pesos!&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;tu total es de: &quot; &amp; precioDONAS*numDONAS &amp; &quot; pesos!&quot;" newline="True"/>
                </else>
            </if>
        </body>
