            <declare name="numDONAS" type="Integer" array="False" size=""/>
            <assign variable="numDONAS" expression="0"/>
            <output expression="&quot;Porfavor ingresa el numero de donas que vas a comprar uwu&quot;" newline="True"/>
            <input variable="numDONAS"/>
